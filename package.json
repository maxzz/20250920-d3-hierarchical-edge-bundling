{
    "name": "d3-hierarchical-edge-bundling-monorepo",
    "version": "1.0.0",
    "description": "Interactive D3.js Hierarchical Edge Bundling with React 19, Vite, and Tailwind CSS v4",
    "private": true,
    "keywords": [
        "d3.js",
        "hierarchical-edge-bundling",
        "react",
        "vite",
        "tailwind",
        "typescript",
        "pnpm",
        "monorepo"
    ],
    "author": "",
    "license": "MIT",
    "engines": {
        "node": ">=18.0.0",
        "pnpm": ">=8.0.0"
    },
    "packageManager": "pnpm@8.15.0",
    "devDependencies": {
        "@types/node": "^20.14.0",
        "tsx": "^4.20.5",
        "typescript": "^5.5.0"
    },
    "scripts": {
        "dev": "pnpm --color --filter web dev",
        "build": "pnpm update-env && pnpm --filter web build",
        "preview": "pnpm --filter web preview",
        "update-env": "tsx scripts/update-env.ts"
    }
